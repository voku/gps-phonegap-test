<?xml version='1.0' encoding='utf-8'?>
<widget id="com.test.app.example.geolocation" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <preference name="loadUrlTimeoutValue" value="180000" />
    <feature name="jsHybugger">
        <param name="android-package" value="org.jshybugger.cordova.JsHybuggerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>GeoLocation Test</name>
    <description>
        This is a cool GeoLocation Example app.
    </description>
    <author email="test@test.com" href="http://test.com">
        only a test
    </author>
    <feature name="http://api.phonegap.com/1.0/device" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="auto-hide-splash-screen" value="false" />
    <access origin="*" />
</widget>
